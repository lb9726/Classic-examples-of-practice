


2进制转10进制类似


10进制转2进制
#include<stdio.h>
int main()
{
   int num,num1,num2,n=0,i,b[33];
   printf("请输入num的值\n");
   scanf("%d",&num);
   while(num)
   {
     b[n++]=num%2;
     num=num/2;
   }
   // b[n]=1;
   printf("10进制对应的2进制数为：\n");
   for(i=n-1;i>=0;i--)
       {
          printf("%d",b[i]);
       }
       printf("\n");
   return 0;
}








